{
  "rules": {
    "allocator-first-param": "warn",
    "avoid-as": "warn",
    "case-convention": "warn",
    "empty-file": "warn",
    "homeless-try": "error",
    "must-return-ref": "error",
    "no-catch-return": "warn",
    "no-unresolved": "error",
    "returned-stack-reference": "warn",
    "suppressed-errors": "error",
    "unsafe-undefined": "error",
    "unused-decls": "warn",
    "useless-error-return": "error"
  }
}
