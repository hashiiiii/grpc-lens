{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "textlint": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "textlint",
        "--mcp"
      ],
      "env": {}
    },
    "godoc": {
      "type": "stdio",
      "command": "godoc-mcp",
      "args": [],
      "env": {
        "GOPATH": "${env:GOPATH}",
        "GOMODCACHE": "${env:GOMODCACHE}"
      }
    },
    "zig-docs": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "zig-mcp@latest",
        "--version",
        "master",
        "--update-policy",
        "manual"
      ]
    }
  }
}
